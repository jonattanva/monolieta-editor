{
  "name": "monolieta-editor",
  "version": "0.0.1",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cypress": "cypress open",
    "dev": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "storybook": "npm run start --prefix ./.storybook",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run"
  },
  "devDependencies": {
    "@cypress/code-coverage": "~3.10.8",
    "@sveltejs/adapter-auto": "~2.1.0",
    "@sveltejs/kit": "~1.22.1",
    "@testing-library/cypress": "~9.0.0",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/svelte": "~4.0.3",
    "@types/cookie": "~0.5.1",
    "@typescript-eslint/eslint-plugin": "~5.61.0",
    "@typescript-eslint/parser": "~5.61.0",
    "@vitest/coverage-v8": "~0.33.0",
    "autoprefixer": "~10.4.14",
    "cypress": "~12.17.0",
    "cypress-file-upload": "~5.0.8",
    "eslint": "~8.44.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-cypress": "~2.13.3",
    "eslint-plugin-svelte": "~2.32.2",
    "jsdom": "~22.1.0",
    "postcss": "~8.4.25",
    "prettier": "~2.8.8",
    "prettier-plugin-svelte": "~2.10.1",
    "prettier-plugin-tailwindcss": "~0.3.0",
    "svelte": "~4.0.5",
    "svelte-check": "~3.4.5",
    "svelte-preprocess": "~5.0.4",
    "tailwindcss": "~3.3.2",
    "tslib": "~2.6.0",
    "typescript": "~5.1.6",
    "vite": "~4.4.8",
    "vitest": "~0.33.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "~5.0.5",
    "@fontsource/lato": "~5.0.5",
    "monolieta-search": "~1.2.0",
    "nanoid": "~4.0.2",
    "svelte-tiny-virtual-list": "~2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonattanva/monolieta-editor.git"
  },
  "engines": {
    "node": ">=16.x"
  }
}
